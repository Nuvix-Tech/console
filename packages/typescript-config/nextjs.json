{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "module": "ESNext",
    "target": "es2024",
    "moduleResolution": "bundler",
    "allowJs": true,
    "noEmit": true
  }
}
