{
  "name": "nuvix-console",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "biome format --write ./"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@nuvix/eslint-config": "workspace:*",
    "@nuvix/typescript-config": "workspace:*",
    "turbo": "latest",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@parcel/watcher",
      "core-js-pure",
      "esbuild",
      "sharp"
    ],
    "ignoredBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
